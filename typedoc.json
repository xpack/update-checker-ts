{
  /* https://typedoc.org/guides/options/ */
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": true,
  "customCss": "./assets/css/custom.css",
  "entryPoints": [
    "./src/index.ts"
  ],
  "entryPointStrategy": "resolve",
  "exclude": [
    "**/*.d.ts",
    "tests/**",
    "node_modules/**"
  ],
  "githubPages": true,
  "includeVersion": true,
  "intentionallyNotExported": [
    "Logger",
    "UpdateCheckerTestEnvironment"
  ],
  "navigationLinks": {
    "xPack": "https://xpack.github.io/",
    "GitHub": "https://github.com/xpack/update-checker-ts/",
    "Org": "https://github.com/xpack/"
  },
  "out": "docs",
  "readme": "./README.md",
  "sidebarLinks": {
  },
  "sort": [
    "instance-first",
    "visibility"
  ],
  "tsconfig": "./src/tsconfig.json"
}
